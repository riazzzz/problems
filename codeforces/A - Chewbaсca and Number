#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define PI      acos(-1.0)
#define all(x) x.begin(),x.end()
#define nl      '\n' 


//https://codeforces.com/contest/514/problem/A

//////////////code

void solve()
{

string s;
cin>>s; 
int n=s.size();

 if(s[0]>='5'&&s[0]!='9'){
    s[0]=(char)('9'-s[0]+'0');



}
for(int i=1;i<n;i++){
if(s[i]>='5'){
    s[i]=(char)('9'-s[i]+'0');}

}

cout<<s<<nl;



/////////////////////endcode;
}





int main()
{
 //{
  ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  #ifndef ONLINE_JUDGE
freopen("input.txt", "r", stdin);

freopen("output.txt", "w", stdout);
#else
// online submission
#endif
//}




int t;
t=1;

while(t--){


 //////////////////////////////////codee 
solve();


}


return 0;


}
